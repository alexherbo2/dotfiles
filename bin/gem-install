#!/bin/sh

nix-shell-install-rails

nix-shell --packages postgresql_12 --run 'gem install pg'
nix-shell --packages sqlite --run 'gem install sqlite3'

gem install activerecord
gem install byebug
gem install faker
gem install pry
gem install pry-byebug
gem install rake
gem install rspec
gem install rubocop
gem install sinatra
