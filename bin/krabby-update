#!/bin/sh

cd ~/.config/krabby/packages
cp ~/projects/krabby/src/krabby.js .
cp ~/projects/krabby/src/krabby/init.js krabby
cp ~/projects/krabby/src/krabby.css .
cp ~/projects/modal.js/scripts/modal.js .
cp ~/projects/prompt.js/scripts/prompt.js .
cp ~/projects/hint.js/scripts/hint.js .
cp ~/projects/mark.js/scripts/mark.js .
cp ~/projects/selection.js/scripts/selection.js .
cp ~/projects/selection.js/styles/selection.css .
cp ~/projects/mouse.js/scripts/mouse.js .
cp ~/projects/clipboard.js/scripts/clipboard.js .
cp ~/projects/scroll.js/scripts/scroll.js .
cp ~/projects/player.js/scripts/player.js .
cp ~/projects/krabby-selection/scripts/krabby/selection.js krabby
cp ~/projects/krabby-selection/styles/krabby/selection.css krabby
cp ~/projects/krabby-launchlet/scripts/krabby/launchlet.js krabby

cd ~/.config/krabby
make -o fetch

cd ~/projects/krabby/website/packages
cp ~/projects/krabby/src/krabby.css .
cp ~/projects/modal.js/scripts/modal.js .
cp ~/projects/prompt.js/scripts/prompt.js .
cp ~/projects/hint.js/scripts/hint.js .
cp ~/projects/mark.js/scripts/mark.js .
cp ~/projects/selection.js/scripts/selection.js .
cp ~/projects/selection.js/styles/selection.css .
cp ~/projects/mouse.js/scripts/mouse.js .
cp ~/projects/clipboard.js/scripts/clipboard.js .
cp ~/projects/scroll.js/scripts/scroll.js .
cp ~/projects/player.js/scripts/player.js .
cp ~/projects/carousel.js/scripts/carousel.js .
