command=$XDG_CONFIG_HOME/i3blocks/blocklets/$BLOCK_NAME
interval=once

[clipboard]
interval=5

[net-speed enp8s0]
label=⌥
interval=5

[net-speed wlp7s0]
label=
interval=5

[gmail]
command=user=alexherbo2 password=$(get-password alexherbo2@gmail.com) $XDG_CONFIG_HOME/i3blocks/blocklets/gmail
label=
interval=30

[temperature]
label=
interval=30

[disk /]
label=
