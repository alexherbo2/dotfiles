# Configure ControlMaster for multiplexing
# https://man.openbsd.org/ssh_config#ControlMaster
Host *
  ControlMaster auto
  ControlPath ~/.cache/ssh/sessions/%r@%h:%p
  ControlPersist yes

Host mawww
  User taupiqueur
  HostName tamiok.kakoune.org

Host paris8
  User ili900
  HostName bocal.cs.univ-paris8.fr
